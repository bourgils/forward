{
  "name": "@bourgils/forward",
  "version": "0.1.1",
  "description": "A fast, temporary CLI runtime for JavaScript projects. Run commands without polluting your project.",
  "bin": {
    "fwd": "./bin/fwd.js"
  },
  "type": "module",
  "main": "src/cli/index.js",
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "dev": "node bin/fwd.js",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "keywords": [
    "cli",
    "dev",
    "tooling",
    "vite",
    "nextjs",
    "zero-install",
    "temporary",
    "runtime",
    "project-runner",
    "npm",
    "sandbox",
    "isolated",
    "scripts",
    "session",
    "pipe",
    "frontend",
    "developer-experience"
  ],
  "author": "bourgils",
  "license": "MIT",
  "dependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "pretty-bytes": "^6.1.1",
    "prompts": "^2.4.2"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "eslint": "^9.24.0",
    "prettier": "^3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bourgils/forward.git"
  },
  "bugs": {
    "url": "https://github.com/bourgils/forward/issues"
  },
  "homepage": "https://github.com/bourgils/forward#readme"
}
